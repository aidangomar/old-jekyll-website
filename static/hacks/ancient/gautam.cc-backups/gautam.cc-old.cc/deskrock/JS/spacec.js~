function  calSpace() {
var weightcalc = prompt ("What's your Weight?", "Type Here!");

if (weightcalc) {
var Moon = 0.167;
var Mercury = 0.284;
var Venus = 0.907;
var Mars = 0.38;
var Jupiter = 2.34;
var Uranus = 0.795;
var Saturn = 1.108;
var Neptune = 1.125;
var Pluto = 0.041;

var m = (weightcalc*Moon);
var me = (weightcalc*Mercury);
var ve = (weightcalc*Venus);
var ma = (weightcalc*Mars);
var ju = (weightcalc*Jupiter);
var u = (weightcalc*Uranus);
var sa = (weightcalc*Saturn);
var ne = (weightcalc*Neptune);
var plu = (weightcalc*Pluto);


alert('Your Weight on the Moon is "+ m +"lbs'); 
alert('Your Weight on Mercury is "+ me +"lbs');
alert('Your Weight on Venus is "+ ve +"lbs');
alert('Your Weight on Mars is "+ ma +"lbs');
alert('Your Weight on Jupiter is "+ ju +"lbs');
alert('Your Weight on Uranus is "+ u +"lbs');
alert('Your Weight on Saturn is "+ sa +"lbs');
alert('Your Weight on Neptune is "+ ne +"lbs');
alert('Your Weight on Pluto is "+ plu +"lbs');




}


}