var quotes = [
    ['Yesterday is history. Tomorrow is a mystery. But tomorrow is a gift, that is why it's called the present.', 'Master Oogway'],
    ['Do or do not. There is no try.', 'Master Yoda'],
    ['Be the change you wish to see in the world.', 'Mahatma Gandhi'],
    ['It is not enough to be compassionate, we must act.', 'Dalai Lama'],
    ['Quality is more important than quantity. One home run is much better than two doubles.', 'Steve Jobs'],
    ['I fear not the man who has practiced 10,000 kicks once, but I fear the man who has practiced one kick 10,000 times.', 'Bruce Lee']
];

var quote = quotes[Math.floor(Math.random() * quotes.length)];
document.getElementByClass("quote").innerHTML = quote[0] + " --" + quote[1];
