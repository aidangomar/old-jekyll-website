         $("#homeNav").click(function() {
	 var folded = new OriDomi(document.getElementsByClassName('container')[0]);
	 folded.foldUp();
         setTimeout(function() {
         location.href = '/../revamp';
         }, 500);
	 });

	 $("#aboutNav").click(function() {
	 //do absolutely nothing!

	 });

	 $("#projectsNav").click(function() {
	 var folded = new OriDomi(document.getElementsByClassName('container')[0]);
	 folded.foldUp();
	 setTimeout(function() {
	 location.href = '/../revamp/projects';
	 }, 500);

	 });

	 $("#contactNav").click(function() {
