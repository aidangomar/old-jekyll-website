    <script>
  
  // $(.appIcon).click(function() {
  //   $(.appIcon).style = ""
  // });

  var iphoneColor = "black";

  $(document).ready(function () {
  startTime();
  prettify();

  $("#appWrapper").hide();
  setTimeout(awesomeStartup, 800);
  // $("#appWrapper").effect("bounce");
  // setTimeout(function(){startTime()},500);

  });

  function awesomeStartup() {
    $("#appWrapper").effect("slide", "slow");
    $("#footerMenuArea").html("<hr class=\"style-one\"><a href=\"mailto:gautam@mittal.net\" class=\"flatButton flatRed\">Want a cool website like this?</a><a href=\"https://www.twitter.com/gthinkin/\" class=\"flatButton flatBlue\">Follow @gthinkin</a><br /><br /><span style=\"color:black;\">Copyright 2013 Gautam Mittal. All rights reserved.</span>").hide().fadeIn();
    
  }
  
  function prettify() {
    var chooseWrapper = Math.random();
    if (chooseWrapper <= 0.50) {
      $("#appWrapper").css( "background-image", "url('portfolio-media/ipod4.png')" );
      iphoneColor = "white";
    } else {
      $("#appWrapper").css( "background-image", "url('portfolio-media/ipod4.png')" );
      // $("#appWrapper").css( "background-image", "url('portfolio-media/iphone4.png')" );
      iphoneColor = "black";
    }

    var chooseWallpaper = Math.random();
    if (chooseWallpaper <= 0.25) {
      $("#appContent").css( "background-image", "url('portfolio-media/mav.jpg')" );
    } else if (chooseWallpaper > 0.25 && chooseWallpaper < 0.50) {
      $("#appContent").css( "background-image", "url('portfolio-media/mav.jpg')" );
    } else if (chooseWallpaper > 0.50 && chooseWallpaper < 0.75) {
      $("#appContent").css( "background-image", "url('portfolio-media/mav.jpg')" );
    } else {
      $("#appContent").css( "background-image", "url('portfolio-media/mav.jpg')" );
    }
  }

  $("#homeButton").dblclick(function() {
      if (iphoneColor == "white") {
        $("#appWrapper").css( "background-image", "url('portfolio-media/iphone4.png')" );
        iphoneColor = "black";
      } else {
        $("#appWrapper").css( "background-image", "url('portfolio-media/ipod4.png')" );
        iphoneColor = "white";
      }
  });

  $('#about').click(function () {
  $('#aboutModal').modal({
  keyboard:true
  });
  });
  
  $('#projects').click(function () {
