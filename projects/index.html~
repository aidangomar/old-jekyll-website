  function startTime () {
  var today = new Date();
  var h = today.getHours();
  var m = today.getMinutes();
  var s = today.getSeconds();
  var amPm = "AM";
  m = checkTime(m);
  s = checkTime(s);

  if (h >= 12) {
  amPm = "PM";
  } else {
  amPm = "AM";
  }

  if (h>12) {
  h = h-12;
  }

  if (h == 0) {
  h = 12;
  }


  document.getElementById('time').innerHTML=h+"<span class='blue'>:</span>"+m+" "+amPm;

  t=setTimeout(function(){startTime()},500);

  }
  
  function checkTime (i) {
  if (i < 10) {
    i = "0"+i;
      }
        return i;
  }
