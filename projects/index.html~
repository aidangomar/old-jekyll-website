	 var folded = new OriDomi(document.getElementsByClassName('container')[0]);
	 folded.foldUp();
         setTimeout(function() {
         location.href = '/../revamp';
         }, 500); 
	 });

	 $("#aboutNav").click(function() {
	 var folded = new OriDomi(document.getElementsByClassName('container')[0]);
	 folded.foldUp();
